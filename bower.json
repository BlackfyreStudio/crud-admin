{
  "name": "crud-admin",
  "private": true,
  "dependencies": {
    "bootstrap-sass": "~3.3.5",
    "moment": "^2.13.0",
    "eonasdan-bootstrap-datetimepicker": "^4.17.37",
    "font-awesome": "^4.6.3",
    "simplemde": "^1.10.1",
    "sweetalert": "^1.1.3",
    "bootbox.js": "bootbox#^4.4.0",
    "bootstrap-sortable": "^2.0.0",
    "underscore": "^1.8.3",
    "bootstrap-maxlength": "^1.7.0",
    "select2": "^4.0.3",
    "select2-bootstrap-theme": "^0.1.0-beta.7"
  },
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/_bootstrap.scss",
        "assets/fonts/bootstrap/*",
        "assets/javascripts/bootstrap.js"
      ]
    },
    "eonasdan-bootstrap-datetimepicker": {
      "main": [
        "src/js/bootstrap-datetimepicker.js",
        "src/sass/bootstrap-datetimepicker-build.scss"
      ]
    },
    "moment-timezone": {
      "main": [
        "builds/moment-timezone-with-data.js"
      ]
    },
    "font-awesome": {
      "main": [
        "scss/font-awesome.scss",
        "fonts/*"
      ]
    },
    "select2": {
      "dependencies": {
        "jquery": "~2.1.4"
      },
      "main": [
        "dist/js/select2.full.js",
        "src/scss/core.scss"
      ]
    },
    "select2-bootstrap-theme": {
      "main": [
        "src/select2-bootstrap.scss"
      ]
    },
    "simplemde": {
      "main": [
        "dist/simplemde.min.js",
        "dist/simplemde.min.css"
      ]
    },
    "bootbox.js": {
      "dependencies": {
        "bootstrap-sass": "~3.3.5"
      }
    },
    "sweetalert": {
      "dependencies": {
        "jquery": "~2.1.4",
        "bootstrap-sass": "~3.3.5"
      },
      "main": [
        "dist/sweetalert-dev.js",
        "dev/loader-animation.css",
        "dev/sweetalert.scss"
      ]
    },
    "bootstrap-maxlength": {
      "main": [
        "src/bootstrap-maxlength.js"
      ]
    }
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.5.3"
  }
}
